[SSLConf]
Version = Compatible_client
VerifyMode = CLIENT_AUTH_REQUIRE # CLIENT_AUTH_NONE, CLIENT_AUTH_REQUEST, CLIENT_AUTH_REQUIRE
VerifyDepth = 9;
CipherSuite = ALL:!LOW:!EXP:!MD5:!aNULL:@STRENGTH

[Verification]
OwnCert = client.pem
OwnPrivateKey = clientprk.pem
CAFile = rootcert.pem
CADir =
HostList = splat.zork.org