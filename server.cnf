# Version for server: SSLv3_server, TLSv1_server, TLSv1.1_server, TLSv1.2_server
# or Compatible_server (support all the four versions).
Version = Compatible_server

# AUTH_NONE, AUTH_REQUEST, AUTH_REQUIRE
CertificateVerification = AUTH_REQUIRE

CipherSuite = ALL:!LOW:!EXP:!MD5:!aNULL:@STRENGTH

CAFile = rootCAcert.pem

CRLFile = crl.pem

OwnCert = servercert.pem

OwnPrivateKey = serverkey.pem

AcceptableSubjectAltName = *@easyssl.com 