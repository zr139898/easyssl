[SSLConf]
Version = Compatible_server # SSLv3, TLSv1, TLSv1.1, TLSv1.2, Compatible
Authentication = AUTH_REQUIRE # AUTH_NONE, AUTH_REQUEST, AUTH_REQUIRE
CipherSuite = ALL:!LOW:!EXP:!MD5:!aNULL:@STRENGTH

[Verification]
OwnCert = servercert.pem
OwnPrivateKey = serverkey.pem
CAFile = rootCAcert.pem
CRLFile = crl.pem
AcceptableSubjectAltName = *@zork.org